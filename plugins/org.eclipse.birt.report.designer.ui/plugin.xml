<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <!-- Extension points -->
   <extension-point id="odadatasource" name="%extension.odadatasource" schema="schema/datasource.exsd"/>
   <extension-point id="reportitemUI" name="%designer.ui.extensionPointName" schema="schema/reportitemUI.exsd"/>
   <extension-point id="menuBuilders" name="%extension.menuBuilders" schema="schema/menuBuilders.exsd"/>
   <extension-point id="elementAdapters" name="elementAdapters" schema="schema/elementAdapters.exsd"/>
   

   
   <!-- ActionSets -->
   <extension
         point="org.eclipse.ui.actionSets">      
      <actionSet
            id="org.eclipse.birt.report.designer.ui.publishActionSet"
            label="%ActionSet.Publish"
            visible="false">
         <action
               class="org.eclipse.birt.report.designer.ui.actions.PublishTemplateAction"
               id="org.eclipse.birt.report.designer.ui.TemplatepublisAction"
               label="%publish.template"
               menubarPath="file/export"
               style="push"/>
         <action
               class="org.eclipse.birt.report.designer.ui.actions.PublishLibraryAction"
               id="org.eclipse.birt.report.designer.ui.LibraryPublisAction"
               label="%publish.library"
               menubarPath="file/export"
               style="push"/>
      </actionSet>
   </extension>
   
   <!-- Popup menu -->

   <!-- Preference pages --> 
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.birt.report.designer.ui.preferences.GeneralPreferencePage"
            name="%preference.birt"
            id="org.eclipse.birt.report.designer.ui.preferences"/>
      <page
            class="org.eclipse.birt.report.designer.ui.preferences.ElementNamesPreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.elementnames"
            id="org.eclipse.birt.report.designer.ui.preferences.ElementNamesPreferencePage"/>
       <page
            class="org.eclipse.birt.report.designer.ui.preferences.TemplatePreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.template"
            id="org.eclipse.birt.report.designer.ui.preferences.TemplatePreferencePage"/>
       <page
             category="org.eclipse.birt.report.designer.ui.preferences"
             class="org.eclipse.birt.report.designer.ui.preferences.ResourcePreferencePage"
             id="org.eclipse.birt.report.designer.ui.preferences.ResourcePreferencePage"
             name="%preference.resource"/>           
       <page
            class="org.eclipse.birt.report.designer.ui.preferences.DateSetPreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.datasetpreview"
            id="org.eclipse.birt.report.designer.ui.preferences.DateSetPreferencePage"/>    
       <page
            class="org.eclipse.birt.report.designer.ui.preferences.CommentTemplatesPreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.commenttemplates"
            id="org.eclipse.birt.report.designer.ui.preferences.CommentTemplatesPreferencePage"/>                        
   </extension>

   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            natureId="org.eclipse.birt.report.designer.ui.reportprojectnature"
            icon="icons/ovr16/report_project_over.gif"
            id="org.eclipse.birt.report.designer.ui.projectimage"/>
   </extension>
   <extension
         id="org.eclipse.birt.report.designer.ui.reportprojectnature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run class="org.eclipse.birt.report.designer.ui.ProjectNature"/>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.team.core.fileTypes">
      <fileTypes
            extension="rptdesign"
            type="text"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSetPartAssociations">
      <actionSetPartAssociation targetID="org.eclipse.birt.report.designer.ui.publishActionSet">
         <part id="org.eclipse.birt.report.designer.ui.editors.TemplateEditor"/>
         <part id="org.eclipse.birt.report.designer.ui.editors.LibraryEditor"/>
         <part id="org.eclipse.birt.report.designer.ui.editors.ReportEditor"/>
      </actionSetPartAssociation>
   </extension>
</plugin>
