<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <!-- Extension points -->
   <extension-point id="odadatasource" name="%extension.odadatasource" schema="schema/datasource.exsd"/>
   <extension-point id="reportitemUI" name="%designer.ui.extensionPointName" schema="schema/reportitemUI.exsd"/>
   <extension-point id="menuBuilders" name="%extension.menuBuilders" schema="schema/menuBuilders.exsd"/>
   <extension-point id="elementAdapters" name="elementAdapters" schema="schema/elementAdapters.exsd"/>
   <extension-point id="reportItemEditpart" name="BIRT Designer Report Item EditPart extension" schema="schema/reportItemEditpart.exsd"/>
   <extension-point id="DNDServices" name="DND Services" schema="schema/DNDServices.exsd"/>

   
   <!-- ActionSets -->
   <extension
         point="org.eclipse.ui.actionSets">      
      <actionSet
            id="org.eclipse.birt.report.designer.ui.publishActionSet"
            label="%ActionSet.Publish"
            visible="false">
         <action
               class="org.eclipse.birt.report.designer.ui.actions.PublishTemplateAction"
               id="org.eclipse.birt.report.designer.ui.TemplatepublisAction"
               label="%publish.template"
               menubarPath="file/export"
               style="push"/>
         <action
               class="org.eclipse.birt.report.designer.ui.actions.PublishLibraryAction"
               id="org.eclipse.birt.report.designer.ui.LibraryPublisAction"
               label="%publish.library"
               menubarPath="file/export"
               style="push"/>
      </actionSet>
   </extension>
   
   <!-- Popup menu -->

   <!-- Preference pages --> 
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.birt.report.designer.ui.preferences.GeneralPreferencePage"
            name="%preference.birt"
            id="org.eclipse.birt.report.designer.ui.preferences"/>
      <page
            class="org.eclipse.birt.report.designer.ui.preferences.ElementNamesPreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.elementnames"
            id="org.eclipse.birt.report.designer.ui.preferences.ElementNamesPreferencePage"/>
       <page
            class="org.eclipse.birt.report.designer.ui.preferences.TemplatePreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.template"
            id="org.eclipse.birt.report.designer.ui.preferences.TemplatePreferencePage"/>
       <page
             category="org.eclipse.birt.report.designer.ui.preferences"
             class="org.eclipse.birt.report.designer.ui.preferences.ResourcePreferencePage"
             id="org.eclipse.birt.report.designer.ui.preferences.ResourcePreferencePage"
             name="%preference.resource"/>           
       <page
            class="org.eclipse.birt.report.designer.ui.preferences.DateSetPreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.datasetpreview"
            id="org.eclipse.birt.report.designer.ui.preferences.DateSetPreferencePage"/>    
       <page
            class="org.eclipse.birt.report.designer.ui.preferences.CommentTemplatesPreferencePage"
            category="org.eclipse.birt.report.designer.ui.preferences"
            name="%preference.commenttemplates"
            id="org.eclipse.birt.report.designer.ui.preferences.CommentTemplatesPreferencePage"/>                        
   </extension>

   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            natureId="org.eclipse.birt.report.designer.ui.reportprojectnature"
            icon="icons/ovr16/report_project_over.gif"
            id="org.eclipse.birt.report.designer.ui.projectimage"/>
   </extension>
   <extension
         id="org.eclipse.birt.report.designer.ui.reportprojectnature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run class="org.eclipse.birt.report.designer.ui.ProjectNature"/>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.team.core.fileTypes">
      <fileTypes
            extension="rptdesign"
            type="text"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSetPartAssociations">
      <actionSetPartAssociation targetID="org.eclipse.birt.report.designer.ui.publishActionSet">
         <part id="org.eclipse.birt.report.designer.ui.editors.TemplateEditor"/>
         <part id="org.eclipse.birt.report.designer.ui.editors.LibraryEditor"/>
         <part id="org.eclipse.birt.report.designer.ui.editors.ReportEditor"/>
      </actionSetPartAssociation>
   </extension>
   <!-- used by extend item, for example: chart -->
   <extension
      	point="org.eclipse.core.expressions.propertyTesters">
   		<propertyTester
         	class="org.eclipse.birt.report.designer.internal.ui.extension.PropertyHandlePropertyTester"
         	id="ProertyHandleTest"
         	namespace="PropertyHandle"
         	properties="handleProperty"
         	type="org.eclipse.birt.report.model.api.PropertyHandle">
   		</propertyTester>
   		<propertyTester
         	class="org.eclipse.birt.report.designer.internal.ui.extension.ExtendItemHandlePropertyTester"
         	id="ExtendItemHandle"
         	namespace="ExtendItemHandle"
         	properties="extensionName"
         	type="org.eclipse.birt.report.model.api.ExtendedItemHandle">
   		</propertyTester>
   		<propertyTester
         	class="org.eclipse.birt.report.designer.internal.ui.extension.ElementContainerTester"
         	id="ElementContainerTest"
         	namespace="ElementContainer"
         	properties="containerName"
         	type="org.eclipse.birt.report.model.api.DataItemHandle">
   		</propertyTester>
	</extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.InsertHandler"
            id="org.eclipse.birt.report.designer.ui.command.insertCommand"
            name="insertCommand">
      </command>
      <commandParameterType
            converter="org.eclipse.birt.report.designer.internal.ui.command.SlotHandleConverter"
            id="slothandle"/>
      <commandParameterType
            converter="org.eclipse.birt.report.designer.internal.ui.command.StringConverter"
            id="string"/>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.CopyHandler"
            id="org.eclipse.birt.report.designer.ui.command.copyAction"
            name="copyAction">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.PasteHandler"
            id="org.eclipse.birt.report.designer.ui.command.pasteAction"
            name="pasteAction">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.DeleteHandler"
            id="org.eclipse.birt.report.designer.ui.command.deleteCommand"
            name="deleteCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.CutHandler"
            id="org.eclipse.birt.report.designer.ui.command.cutCommand"
            name="cutCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.InsertRowHandler"
            id="org.eclipse.birt.report.designer.ui.command.insertRowCommand"
            name="insertRowCommand">
      </command>
      <commandParameterType
            converter="org.eclipse.birt.report.designer.internal.ui.command.IntegerConverter"
            id="integer">
      </commandParameterType>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.InsertColumnHandler"
            id="org.eclipse.birt.report.designer.ui.command.insertColumnCommand"
            name="insertColumnCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.MergeHandler"
            id="org.eclipse.birt.report.designer.ui.command.mergeCommand"
            name="mergeCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.SplitCellHandler"
            id="org.eclipse.birt.report.designer.ui.command.splitCommand"
            name="splitCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.CreatePlaceHolderHandler"
            id="org.eclipse.birt.report.designer.ui.command.createPlaceHolderCommand"
            name="createPlaceHolderCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.RevertToReportItemHandler"
            id="org.eclipse.birt.report.designer.ui.command.revertToReportItemCommand"
            name="revertToReportItemCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.RevertToTemplateHandler"
            id="org.eclipse.birt.report.designer.ui.command.revertToTemplateCommand"
            name="revertToTemplateCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.ApplyStyleHandler"
            id="org.eclipse.birt.report.designer.ui.command.applyStyleCommand"
            name="applyStyleCommand">
      </command>
      <commandParameterType
            converter="org.eclipse.birt.report.designer.internal.ui.command.BooleanConverter"
            id="boolean">
      </commandParameterType>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.EditGroupHandler"
            id="org.eclipse.birt.report.designer.ui.command.editGroupCommand"
            name="editGroupCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.InsertGroupHanlder"
            id="org.eclipse.birt.report.designer.ui.command.insertGroupCommand"
            name="insertGroupCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.NewStyleHandler"
            id="org.eclipse.birt.report.designer.ui.command.newStyleCommand"
            name="newStyleCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.EditStyleHandler"
            id="org.eclipse.birt.report.designer.ui.command.editStyleCommand"
            name="editStyleCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.ImpotCSSStyleHandler"
            id="org.eclipse.birt.report.designer.ui.command.importCSSCommand"
            name="importCSSCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.RefreshLibraryHandler"
            id="org.eclipse.birt.report.designer.ui.command.refreshLibraryCommand"
            name="refreshLibraryCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.ExportLibraryHandler"
            id="org.eclipse.birt.report.designer.ui.command.exportLibraryCommand"
            name="exportLibraryCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.PublishToTemplateHandler"
            id="org.eclipse.birt.report.designer.ui.command.publishToTemplateCommand"
            name="publishToTemplateCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.PublishToLibraryHandler"
            id="org.eclipse.birt.report.designer.ui.command.publishToLibraryCommand"
            name="publishToLibraryCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.EditHandler"
            id="org.eclipse.birt.report.designer.ui.command.editCommand"
            name="editCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.BaseInsertHandler"
            id="org.eclipse.birt.report.designer.ui.command.baseInsertCommand"
            name="baseInsertCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.InsertExpressionHandler"
            id="org.eclipse.birt.report.designer.ui.command.insertExpressionCommand"
            name="insertExpressionCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.DeleteRowHandler"
            id="org.eclipse.birt.report.designer.ui.command.deleteRowCommand"
            name="deleteRowCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.DeleteColumnHandler"
            id="org.eclipse.birt.report.designer.ui.command.deleteColumnCommand"
            name="deleteColumnCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.DeleteGrouphandler"
            id="org.eclipse.birt.report.designer.ui.command.deleteGroupCommand"
            name="deleteGroupCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.EditBindingHandler"
            id="org.eclipse.birt.report.designer.ui.command.editBindingCommand"
            name="editBindingCommand">
      </command>
      <command
            defaultHandler="org.eclipse.birt.report.designer.internal.ui.command.ReloadCssStyleHandler"
            id="org.eclipse.birt.report.designer.ui.command.reloadCssStyleCommand"
            name="reloadCssStyleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.birt.report.designer.ui.DNDServices">
      <dropAdapter
            adapter="org.eclipse.birt.report.designer.internal.ui.AggDataDropAdapter">
      </dropAdapter>
   </extension>
   <extension
         point="org.eclipse.birt.report.designer.ui.elementAdapters">
      <adaptable
            class="org.eclipse.birt.report.model.api.DataItemHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.data"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
      <adaptable
            class="org.eclipse.birt.report.model.api.TableHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.table"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
      <adaptable
            class="org.eclipse.birt.report.model.api.ListHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.list"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
      <adaptable
            class="org.eclipse.birt.report.model.api.TextItemHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.text"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
      <adaptable
            class="org.eclipse.birt.report.model.api.TextDataHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.textdata"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
      <adaptable
            class="org.eclipse.birt.report.model.api.ImageHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.image"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
      <adaptable
            class="org.eclipse.birt.report.model.api.ExtendedItemHandle">
         <adapter
               factory="org.eclipse.birt.report.designer.internal.ui.dialogs.BindingDialogFactory"
               id="org.eclipse.birt.report.designer.ui.bindingdialog.extended"
               priority="3"
               singleton="false"
               type="org.eclipse.birt.report.designer.internal.ui.dialogs.IBindingDialogHelper">
         </adapter>
      </adaptable>
   </extension>
</plugin>
